{
  "name": "lambda-spacetalk",
  "version": "1.0.0",
  "description": "Enterprise Lambda backend with Cognito auth and DynamoDB",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist",
    "compile": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "terraform:init": "cd terraform && terraform init",
    "terraform:plan": "cd terraform && terraform plan",
    "terraform:apply": "cd terraform && terraform apply",
    "terraform:destroy": "cd terraform && terraform destroy"
  },
  "keywords": [
    "aws",
    "lambda",
    "cognito",
    "dynamodb",
    "terraform"
  ],
  "author": "Your Company",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "aws-sdk": "^2.1531.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "joi": "^17.12.0",
    "winston": "^3.11.0"
  }
}
